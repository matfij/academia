{
    "name": "express-blueprint",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "ts-node-dev --respawn --poll src/index.ts",
        "build": "tsoa spec-and-routes && tsc",
        "lint": "eslint src/**",
        "gen-api-routes": "tsoa routes",
        "gen-api-spec": "tsoa spec",
        "gen-api-client": "openapi-generator-cli generate -i src/.generated/swagger.json -g typescript-fetch -o .generated"
    },
    "dependencies": {
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.0.4",
        "tsoa": "^6.0.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^20.11.5",
        "@typescript-eslint/eslint-plugin": "^6.18.1",
        "@typescript-eslint/parser": "^6.18.1",
        "eslint": "^8.56.0",
        "nodemon": "^3.0.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3"
    }
}
